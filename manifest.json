{
  "manifest_version": 2,
  "name": "QRL",
  "description": "This extension allows the user to create a QR of the current tab URL",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Generate QRL"
  },
  "permissions": [
    "activeTab"
  ]
}